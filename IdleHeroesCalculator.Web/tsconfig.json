{
  "compilerOptions": {
    "allowJs": true,
    "alwaysStrict": true,
    "module": "es2015",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noEmitOnError": true,
    "removeComments": true,
    "sourceMap": true,
    "strict": true,
    "target": "es5",
    "allowSyntheticDefaultImports": true
  },
  "include": [ "./Scripts/main.ts" ],
  "exclude": [
    "webpack.dev.config.js",
    "webpack.prod.config.js",
    "node_modules",
    "wwwroot"
  ]
}